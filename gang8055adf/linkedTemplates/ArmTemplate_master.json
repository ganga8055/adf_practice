{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gang8055adf"
		},
		"linkedserviceDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedserviceDL'"
		},
		"linkedserviceDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gang8055sa.dfs.core.windows.net/"
		},
		"LoadTriger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/faa80d27-a0dc-4cc0-b9be-e4a41565958f/resourceGroups/RG_ADF_PROJECT/providers/Microsoft.Storage/storageAccounts/gang8055sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gang8055adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedserviceDL_accountKey": {
						"value": "[parameters('linkedserviceDL_accountKey')]"
					},
					"linkedserviceDL_properties_typeProperties_url": {
						"value": "[parameters('linkedserviceDL_properties_typeProperties_url')]"
					},
					"LoadTriger_properties_typeProperties_scope": {
						"value": "[parameters('LoadTriger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}